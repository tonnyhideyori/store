{
  "name": "store",
  "version": "1.0.0",
  "description": "this is web app for inventory/sale management for small shop",
  "main": "index.js",
  "engines": {
    "node": "10.13.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "keywords": [
    "store",
    "duka"
  ],
  "author": "Edwin Ismail",
  "license": "MIT",
  "dependencies": {
    "ajv-errors": "^1.0.1",
    "bcrypt": "^3.0.6",
    "concurrently": "^4.1.0",
    "config": "^3.0.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "joi": "^14.3.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.11",
    "mongoose": "^5.3.14",
    "save": "^2.3.3"
  }
}
